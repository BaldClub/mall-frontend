{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-essential", "standard-with-typescript", "@nuxtjs/eslint-config-typescript"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"],
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["vue"],
  "rules": {},
  "ignorePatterns": ["**/*.json", "**/*.md", "node_modules", "**/*.vue"]
}
